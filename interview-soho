public class Zoho {
    public static void main(final String[] args) {
        final int n = Integer.parseInt(args[0]);
        for(int a=n;a>=1;a--)
        {
            for(int b=n;b>=1;b--)
            {
                if(a>=b)
                {
                    System.out.print(a);
                }
                else
                {
                    System.out.print(b);
                }
            }
            System.out.println();
        }
    }
}
