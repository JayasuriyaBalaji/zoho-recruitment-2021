public class Zoho {
    public static void main(final String[] args) {
        final int n = Integer.parseInt(args[0]);
        for (int i=n; i>0; i--) {
            for (int j=n; j>i; j--) {
                System.out.print(j);
            }

            for (int j=i; j>0; j--){
                System.out.print(i);
            }

            System.out.println();
        }
    }
}
